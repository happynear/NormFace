}

layer {
  name: "slice_eltwise_fc1"
  type: "Slice"
  bottom: "eltwise_fc1"
  top: "eltwise_fc1_orig"
  top: "eltwise_fc1_mirror"
  slice_param {
    axis: 0
 }
}
layer {
  name: "eltmax_eltwise_fc1"
